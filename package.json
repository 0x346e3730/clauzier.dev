    "@fontsource/monaspace-radon": "^5.2.5",
    "@iconify-json/la": "^1.2.1",
    "@iconify-json/ph": "^1.2.2",
    "@iconify/json": "^2.2.427",
    "@tailwindcss/vite": "^4.1.18",
    "astro": "^5.16.9",
    "astro-icon": "^1.1.5",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.14.1",
